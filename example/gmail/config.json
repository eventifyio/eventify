{
    "_comment": "service configuration",
    "name": "gce-vm-collector",
    "image": "gce/vm",
    "driver": "gmail",
    "transport_host": "smtp.gmail.com",
    "pub_options": {
        "acknowledge": true,
        "retain": false,
        "exclude_me": true
    },
    "publish_topic": {
        "topic": "gce-vm",
        "timeout": 20,
        "reply_in": 0
    },
    "subscribed_topics": [
        "gce-vm"
    ],
    "sub_options": {
        "get_retained": false
    },
    "replay_events": false,
    "replay_type": "event_store"
}
